<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                        http://www.springframework.org/schema/context
                        http://www.springframework.org/schema/context/spring-context-3.2.xsd http://www.springframework.org/schema/aop https://www.springframework.org/schema/aop/spring-aop.xsd">

    <bean id="huBaoStudent" class="com.huhu.hm01.model.Student">
        <property name="age" value="18"/>
        <property name="name" value="虎宝"/>
        <property name="subject" value="Java进阶"/>
    </bean>

    <context:component-scan base-package="com.huhu.hm01.service.*"/>
    <!--    <bean id="studentServiceImpl" class="com.huhu.hm01.service.impl.StudentServiceImpl">-->
    <!--    </bean>-->

</beans>